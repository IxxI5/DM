<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Defects Management</title>

    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            @Html.Partial("_NavBar")
            @Html.Partial("_NavDashBoard")
        </div>
    </div>

    @Scripts.Render("~/bundles/jquery")
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.0/js/bootstrap.min.js"></script>
    <!--<script src="~/Scripts/bootstrap.min.js"></script>--> @* Local File does not seem to work -> Use the online Reference (above) *@

    <div class="container body-content">
        @RenderBody() @* Index View *@
        <footer style="text-align: center;">
            <p>&copy; @DateTime.Now.Year - (ASP.NET) Software</p>
        </footer>
    </div>

    @RenderSection("scripts", required: false)

</body>
</html>
